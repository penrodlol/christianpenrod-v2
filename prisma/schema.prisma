generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model PostView {
  id        Int      @id @default(autoincrement())
  createdAt DateTime @default(now())
  post      String   @unique @db.VarChar(255)
  views     Int      @default(0)
}
